      "settings": {
        "foreground": "#f386bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "DOM Variable",
      "scope": ["support.variable.dom.js", "support.variable"],
      "settings": {
        "foreground": "#e8c37d",
        "fontStyle": ""
      }
    },
    {
      "name": "DOM Function",
      "scope": "support.function.dom.js",
      "settings": {
        "foreground": "#53c6ba",
        "fontStyle": ""
